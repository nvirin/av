			<type>string</type>
			<shortdesc>
				<![CDATA[
					Get or set button type.
				]]>
			</shortdesc>
			<longdesc>
				<![CDATA[
					Get or set button type whether button, link or image.
				]]>
			</longdesc>
			
			<defaultvalue>"Button"</defaultvalue>
			<allowedvalues>
			   <value desc="Show edit delete command in button">"Button"</value>
			   <value desc="Show edit delete command in link">"Link"</value>
			   <value desc="Show edit delete command in image">"Image"</value>
			</allowedvalues>			
			
			<samplecode>
<![CDATA[
<?php				
...	
	$col = new GridEditDeleteColumn();
	$col->ButtonType = "Link";
...
?> 
]]>
			</samplecode>
			<supportversion>1.0.0.0</supportversion>
		</property>			


