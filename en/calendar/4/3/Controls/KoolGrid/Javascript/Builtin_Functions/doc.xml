		<method name="grid_sort">
			<type>void</type>
			<parameters>
				<param type="string" optional="false">_column_id</param>
				<param type="int" optional="false">sort_order</param>
				
			</parameters>			
			<shortdesc>
				<![CDATA[
				Sort the column with specified id.
				]]>
			</shortdesc>
			<longdesc>
				<![CDATA[
				Sort the column with specified id. The sort_order have 3 allowed values 0,-1 and 1 standing for "no_sort", "sort desc" and "sort asc" respectively.
				]]>
			</longdesc>
			<samplecode>
				<![CDATA[
<?php
...
	$col = new GridCommandColumn();
	$col->CommandText = "Sort";
	$col->OnClick = "grid_sort(this,1)";//Sort asc;	
	$grid->MasterTable->AddColumn($col);
...
?>
				]]>
			</samplecode>
			<supportversion>1.0.0.0 </supportversion>
		</method> 	
 


