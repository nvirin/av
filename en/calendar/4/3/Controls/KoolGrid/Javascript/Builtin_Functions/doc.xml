	<methods>
		<method name="grid_gopage">
			<type>void</type>
			<parameters>
				<param type="object" optional="false">html_element_object</param>
				<param type="int" optional="false">page_index</param>
			</parameters>			
			<shortdesc>
				<![CDATA[
				Make tableview that contain the element navigate to certain page.
				]]>
			</shortdesc>
			<longdesc>
				<![CDATA[
				Make tableview that contain the element navigate to certain page.
				]]>
			</longdesc>
			<samplecode>
				<![CDATA[
<?php
...
	$col = new GridCommandColumn();
	$col->CommandText = "GoPage";
	$col->OnClick = "grid_gopage(this,3)";	
	$grid->MasterTable->AddColumn($col);
...
?>
				]]>
			</samplecode>
			<supportversion>1.0.0.0 </supportversion>
		</method> 	
 

